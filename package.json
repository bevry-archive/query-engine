  "editions": [
    {
      "description": "coffeescript source code with require for modules",
      "directory": "source",
      "entry": "index.coffee",
      "tags": [
        "coffeescript",
        "require"
      ],
      "engines": false
    },
    {
      "description": "coffeescript compiled for node.js with require for modules",
      "directory": "edition-esnext",
      "entry": "index.js",
      "tags": [
        "javascript",
        "esnext",
        "require"
      ],
      "engines": {
        "node": true,
        "browsers": false
      }
    },
    {
      "description": "coffeescript compiled for browsers with require for modules",
      "directory": "edition-browsers",
      "entry": "index.js",
      "tags": [
        "javascript",
        "require"
      ],
      "engines": {
        "node": false,
        "browsers": "defaults"
      }
    },
    {
      "description": "coffeescript compiled for node.js 13 with require for modules",
      "directory": "edition-node-13",
      "entry": "index.js",
      "tags": [
        "javascript",
        "require"
      ],
      "engines": {
        "node": true,
        "browsers": false
      }
    },
    {
      "description": "coffeescript compiled for node.js 12 with require for modules",
      "directory": "edition-node-12",
      "entry": "index.js",
      "tags": [
        "javascript",
        "require"
      ],
      "engines": {
        "node": true,
        "browsers": false
      }
    },
    {
      "description": "coffeescript compiled for node.js 8 with require for modules",
      "directory": "edition-node-8",
      "entry": "index.js",
      "tags": [
        "javascript",
        "require"
      ],
      "engines": {
        "node": true,
        "browsers": false
      }
    }
  ],
  "main": "index.js",
  "browser": "edition-browsers/index.js",
  "dependencies": {
    "editions": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "assert-helpers": "^5.6.0",
    "backbone": "~1.4.0",
    "benchmark": "~2.1.4",
    "coffeelint": "^2.1.0",
    "exoskeleton": "~0.7.0",
    "kava": "^4.2.0",
    "projectz": "^1.13.0",
    "underscore": "~1.9.1",
    "valid-directory": "^1.4.0"
  },
  "babel": {
    "env": {
      "edition-browsers": {
        "sourceType": "script",
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "defaults",
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          "@babel/proposal-object-rest-spread"
        ]
      },
      "edition-node-13": {
        "sourceType": "script",
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "13"
              },
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          "@babel/proposal-object-rest-spread"
        ]
      },
      "edition-node-12": {
        "sourceType": "script",
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "12"
              },
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          "@babel/proposal-object-rest-spread"
        ]
      },
      "edition-node-8": {
        "sourceType": "script",
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "8"
              },
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          "@babel/proposal-object-rest-spread"
        ]
      }
    }
  },
  "scripts": {
    "our:clean": "rm -Rf ./docs ./edition* ./es2015 ./es5 ./out ./.next",
    "our:compile": "npm run our:compile:edition-browsers && npm run our:compile:edition-esnext && npm run our:compile:edition-node-12 && npm run our:compile:edition-node-13 && npm run our:compile:edition-node-8",
    "our:compile:edition-browsers": "env BABEL_ENV=edition-browsers coffee -bcto ./edition-browsers/ ./source",
    "our:compile:edition-esnext": "coffee -bco ./edition-esnext ./source",
    "our:compile:edition-node-12": "env BABEL_ENV=edition-node-12 coffee -bcto ./edition-node-12/ ./source",
    "our:compile:edition-node-13": "env BABEL_ENV=edition-node-13 coffee -bcto ./edition-node-13/ ./source",
    "our:compile:edition-node-8": "env BABEL_ENV=edition-node-8 coffee -bcto ./edition-node-8/ ./source",
    "our:deploy": "echo no need for this project",
    "our:meta": "npm run our:meta:projectz",
    "our:meta:projectz": "projectz compile",
    "our:release": "npm run our:release:prepare && npm run our:release:check-changelog && npm run our:release:check-dirty && npm run our:release:tag && npm run our:release:push",
    "our:release:check-changelog": "cat ./HISTORY.md | grep v$npm_package_version || (echo add a changelog entry for v$npm_package_version && exit -1)",
    "our:release:check-dirty": "git diff --exit-code",
    "our:release:prepare": "npm run our:clean && npm run our:compile && npm run our:test && npm run our:meta",
    "our:release:push": "git push origin master && git push origin --tags",
    "our:release:tag": "export MESSAGE=$(cat ./HISTORY.md | sed -n \"/## v$npm_package_version/,/##/p\" | sed 's/## //' | awk 'NR>1{print buf}{buf = $0}') && test \"$MESSAGE\" || (echo 'proper changelog entry not found' && exit -1) && git tag v$npm_package_version -am \"$MESSAGE\"",
    "our:setup": "npm run our:setup:install",
    "our:setup:install": "npm install",
    "our:test": "npm run our:verify && npm test",
    "our:verify": "npm run our:verify:coffeelint && npm run our:verify:directory",
    "our:verify:coffeelint": "coffeelint ./source",
    "our:verify:directory": "npx valid-directory",
    "test": "node ./test.js"
  }
}
